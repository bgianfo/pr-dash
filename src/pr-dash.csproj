<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <PublishTrimmed>true</PublishTrimmed>
    <Nullable>enable</Nullable>
    <AnalysisMode>AllEnabledByDefault</AnalysisMode>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.DirectoryServices.AccountManagement" Version="5.0.0" />

    <PackageReference Include="YamlDotNet" Version="9.1.3" />
    <PackageReference Include="Terminal.Gui" Version="0.90.3" />
    <PackageReference Include="Humanizer" Version="2.8.26" />
    <PackageReference Include="Microsoft.TeamFoundationServer.Client" Version="16.178.1-preview" />
    <PackageReference Include="Microsoft.VisualStudio.Services.InteractiveClient" Version="16.178.1-preview" />
    <PackageReference Include="Microsoft.VisualStudio.Services.Client" Version="16.178.1-preview" />
    <PackageReference Include="CommandLineParser" Version="2.8.0" />

    <PackageReference Include="Microsoft.CodeAnalysis.NetAnalyzers" Version="5.0.3">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="3.8.0">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.CodeQuality.Analyzers" Version="3.3.2">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Roslynator.Analyzers" Version="3.0.0">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="AsyncFixer" Version="1.3.0">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="ErrorProne.Net.CoreAnalyzers" Version="0.2.0-beta.7">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="ErrorProne.Net.Structs" Version="0.2.0-beta.7">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="SmartAnalyzers.ExceptionAnalyzer" Version="1.0.10">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>
    <PackageReference Include="SmartAnalyzers.MultithreadingAnalyzer" Version="1.1.31">
      <PrivateAssets>analyzers</PrivateAssets>
    </PackageReference>

    <PackageReference Include="Nerdbank.GitVersioning" Version="3.3.37" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>

</Project>
